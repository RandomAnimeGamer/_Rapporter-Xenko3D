namespace SiliconStudio.Xenko.Rendering.Deferred
{
    /// <summary>
    /// An array of light groups
    /// </summary>
    shader GBuffer : ShaderBase, MaterialPixelStream
    {
        stage override void PSMain()
        {
            base.PSMain();

            streams.ColorTarget = float4(streams.normalWS, 1.0f);
        }
    };
}